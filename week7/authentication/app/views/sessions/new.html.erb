<h1>Log in</h1>
<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :email %><br>
    <%= text_field_tag :email, user_email %>
    </div>
  <div class="field">
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
  </div>
  <div class="actions">
    <%= label_tag "Remember me" %>
    <%= check_box_tag "remember_me" %>
  </div>
  <div class="actions">
    <%= submit_tag "Log In"%>
  </div>
<% end %>
